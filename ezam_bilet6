#include <stdio.h> 
#include <stdlib.h> 
 
#define days 31 
#define p_min 740 
#define p_max 760 
#define p_norm 760 
Int pressure[31]; 
Int main() 
{ 
for(int I = 0; I < days;i++) 
 Pressure[i] = p_min + rand() % (p_max - p_min + 1); 
For(int i = 0; i < days; i++) 
Printf(”%d\t%d”, i + 1,pressure[i]) 
 
Int cnt_blow = 0; 
Int cnt_above = 0; 
Int cnt_exact = 0; 
 
For(int I = 0; I <days; I++) 
{ 
Int v = pressure[i]; 
If(v < p_norm) cnt_bellow++; 
Else if (v > p_norm) cnt_above++; 
Else cnt_exact++; 
} 
 
Printf(“below: %d\n”, cnt_below); 
Printf(“above: %d\n”, cnt_above); 
Printf(“exact: %d\n”, cnt_exact); 
 
return 0 
}